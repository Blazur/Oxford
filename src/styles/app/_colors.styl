createBackgrounds(color, key, val)
  if val
    if key
      a.bg-{color}-{key},
      .bg-{color}-{key}
        background-color val
        color color-white

      if convert(key) < 200
        a.bg-{color}-{key},
        .bg-{color}-{key}
          color color-default
    else
      a.bg-{color},
      .bg-{color}
        background-color val
        color color-white
  else
    a.bg-{color}-{key},
    .bg-{color}-{key}
      color color-default

createButtons(color, key, val)
  if val
    if key
      button[color={color}-{key}],
      button.button-{color}-{key},
      a.button-{color}-{key},
      .button-{color}-{key}
        background-color val
        color color-white
        &:hover,
        &:focus,
        &.active
          background-color val

      if convert(key) < 200
        button[color={color}-{key}],
        button.button-{color}-{key},
        a.button-{color}-{key},
        .button-{color}-{key}
          color color-default

    else
      button[color={color}],
      button.button-{color},
      a.button-{color},
      .button-{color}
        background-color val
        color color-white
        &:hover,
        &:focus,
        &.active
          background-color val

  else
    button[color={color}-{key}],
    button.button-{color}-{key},
    a.button-{color}-{key},
    .button-{color}-{key}
      color color-default

for color, types in material_design_colors
  for key, val in types
    if key is "0"
      define('color-'+color, val)
      createBackgrounds(color, null, val)
      createButtons(color, null, val)

    else
      define('color-'+color+'-'+key, val)
      createBackgrounds(color, key, val)
      createButtons(color, key, val)

    if convert(key) < 200
      createBackgrounds(color, key, null)
      createButtons(color, key, null)



a.bg-default,
.bg-default
  background-color color-white
  color color-grey-500



.button
  display inline-block
  font-size 14px
  font-weight 400
  line-height 1.42857143
  text-align center
  white-space nowrap
  vertical-align middle
  padding 0px 30px
  border none
  margin 10px 1px
  cursor pointer
  border-radius 1px
  -webkit-user-select none
  user-select none
  -webkit-transition box-shadow 0.28s cubic-bezier(0.4, 0, 0.2, 1)
  transition box-shadow 0.28s cubic-bezier(0.4, 0, 0.2, 1)

  &.button-raised
    shadow(1)
    &:focus
      shadow(2)


  &:hover,
  &:focus
    outline none
    border none